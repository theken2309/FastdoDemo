{
  "expo": {
    "name": "Fastdo",
    "slug": "Fastdo",
    "scheme": "Fastdo-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/image/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.fastdo",
      "entitlements": {
        "keychain-access-groups": ["YOUR_TEAM_ID.com.example.fastdo"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.example.fastdo"
      }
    },
    "web": {
      "favicon": "./assets/image/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"]
  }
}
